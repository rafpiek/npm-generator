{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "models": "./src/models",
        "services": "./src/services",
        "utils": "./src/utils",
        "config": "./src/config",
        "routes": "./src/routes",
        "controllers": "./src/controllers",
        "env": "./env.js"
      }
    }]
  ]
}